{
  "name": "Incident Report",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "For incident reporting (Information/Physical).",
  "icons": {
      "16": "icons/SVC Logo 16x16.png",
      "32": "icons/SVC Logo 32x32.png",
      "48": "icons/SVC Logo 48x48.png",
      "128": "icons/SVC Logo 128x128.png"
  },
  "action": {
      "default_popup": "popup/popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup/content.js"]
    }
  ]
}
